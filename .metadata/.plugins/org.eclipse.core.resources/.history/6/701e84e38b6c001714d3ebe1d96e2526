package Dice_Roll;

import java.util.Random;

public class Randomizer {

		int randNum;
		int numRoll;
		int diceType;
		 
		Randomizer(int numRoll, int diceType){
			this.numRoll = numRoll;
			this.diceType = diceType;
			Random rand = new Random();
			randNum = rand.nextInt(diceType) + 1;
		}
	
}
